	agent any
	stages {
		stage('init') {
			steps {
				echo 'Hi, this is Roger Taylor from SkyeNets'
				echo 'We are starting the Testing'
			}
		}
		stage('build') {
			steps {
				echo 'Building Sample Maven Project'
			}
		}
		stage('deploy') {
			steps {
				echo 'Deploying to staging area'
			}
		}
	} 

}